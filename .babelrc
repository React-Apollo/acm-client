{
  "presets": ["babel-preset-expo"],
  "plugins": [
    [
      "babel-plugin-root-import",
      [
        {
          "rootPathPrefix": "~",
          "rootPathSuffix": "App"
        },
        {
          "rootPathPrefix": "@",
        }
      ]
  ],
    [
      "babel-plugin-inline-import",
      {
        "extensions": [".graphql"]
      }
    ]
  ],
  "env": {
    "development": {
      "plugins": ["transform-react-jsx-source"]
    }
  }
}
